<AnchorPane id="AnchorPane" prefHeight="432.0" prefWidth="600.0" style="-fx-background-color: #DCF1F1;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="workbase.FXMLDocumentController">
   <children>
      <Pane prefHeight="460.0" prefWidth="189.0" style="-fx-background-color: #BED1D1;">
         <children>
            <Button layoutX="1.0" layoutY="80.0" mnemonicParsing="false" onAction="#btnSearchClicked" prefHeight="64.0" prefWidth="189.0" style="-fx-background-color: #CAEDED;" text="Підібрати вакансію">
