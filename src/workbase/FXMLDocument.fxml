                  <Cursor fx:constant="HAND" />
               </cursor>
               <font>
                  <Font name="Franklin Gothic Book" size="14.0" />
               </font>
            </Button>
            <MenuBar layoutY="-3.0" prefHeight="32.0" prefWidth="600.0" style="-fx-background-color: #BED1CC; -fx-border-color: #A9BAB6;">
              <menus>
                <Menu mnemonicParsing="false" style="-fx-border-color: #A9BAB6;" text="Додатково">
