                  <Cursor fx:constant="HAND" />
               </cursor>
               <font>
                  <Font name="Franklin Gothic Book" size="14.0" />
               </font>
            </Button>
            <Button layoutX="1.0" layoutY="361.0" mnemonicParsing="false" onAction="#btnExitClicked" prefHeight="64.0" prefWidth="189.0" style="-fx-background-color: #CAEDED;" text="Вихід з програми">
